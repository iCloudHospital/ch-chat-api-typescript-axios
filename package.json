{
  "name": "ch-chat-api-typescript-axios",
  "description": "An OpenAPI generator tool for the CH Chat API, designed to simplify API client generation and streamline integration workflows.",
  "version": "1.4.37",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "commit": "cz",
    "prepare": "husky",
    "generate": "openapi-generator-cli generate && tsc",
    "changeset": "changeset",
    "ci:version": "changeset version",
    "ci:publish": "npm publish --access public",
    "release": "changeset publish"
  },
  "license": "ISC",
  "keywords": [
    "axios",
    "typescript",
    "api generator",
    "chat api",
    "api client",
    "icloudhospital",
    "ch"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.1.0",
    "husky": "^9.1.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icloudhospital/ch-chat-api-typescript-axios"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
